<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.birt.chart.examples"
   name="BIRT Chart Examples Plug-in"
   version="2.0.0"
   provider-name="Eclipse.org"
   class="org.eclipse.birt.chart.examples.ExamplesPlugin">

   <runtime>
      <library name="examples.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.birt.chart.engine"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.birt.core"/>
      <import plugin="org.eclipse.birt.chart.engine.extension"/>
      <import plugin="org.eclipse.birt.chart.device.extension"/>
      <import plugin="org.eclipse.birt.report.model"/>
      <import plugin="org.eclipse.birt.chart.reportitem"/>
      <import plugin="org.eclipse.birt.report.engine"/>
   </requires>
   <extension
         point="org.eclipse.birt.chart.engine.displayservers">
      <displayserver
            server="org.eclipse.birt.chart.device.svg.SVGDisplayServer"
            name="ds.SVG"/>
   </extension>
</plugin>
